@model IEnumerable<GoSharpProject.Models.entities.Order>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New Order", "Index", "Items")
</p>
<table class="table">
    <tr>
        <th>
            Order Number
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DueDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OrderStartus)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DetailDescription)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink( item.Id.ToString(), "Details", new { id = item.Id })
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DueDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OrderStartus)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DetailDescription)
        </td>
       
        <td>
        </td>
    </tr>
}

</table>
